#main{
	margin-top:300px;
   height:770px;
   width:1200px;
   background:url(../img/register/background.jpg) no-repeat;
   margin:0 auto;

 }
 #main div.black{
   width:280px;
   height:320px;
   background-color:rgba(0,0,0,.3);
   margin-left:825px;
   margin-top:310px;
 }

 #main div.black table{
   width:100%;
 }
 #main div.black table td{
   color:#fff;

 }

#main div.black table td a{
   color:#fff;
   text-decoration:none;
   font-size:13px;
 }
#main div.black table td.sp{
 font-size:13px;
}
#uname,#upwd,#btn{
 width:100%;
 height:38px;
 border:0;
 outline:0;
 font-size:15px;
 padding:0 50px 0 15px;
 box-sizing:border-box;
 margin-top:20px;
}

#btn{
 background-image:linear-gradient(to bottom,#FE6D2C 0%,#FE6D2C 100%);
 padding-left:55px;

}
table h4{
 margin:0;
}
#header{
	width:100%;
	height:100px;
}
#footer{
	width:100%;
	height:272px;
	margin-top:10px;
}